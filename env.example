# ComplianceFlow Environment Variables
# Copy this to .env and update with your values
# NEVER commit .env to git!

# Database
DATABASE_URL=postgresql://complianceflow:dev_password_change_me@localhost:5432/complianceflow
DB_HOST=localhost
DB_PORT=5432
DB_NAME=complianceflow
DB_USER=complianceflow
DB_PASSWORD=dev_password_change_me

# Redis
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379

# Kafka
KAFKA_BOOTSTRAP_SERVERS=localhost:9092

# JWT Authentication
JWT_SECRET_KEY=your-secret-key-change-me-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# Application Settings
ENVIRONMENT=development
LOG_LEVEL=INFO
DEBUG=True

# Service URLs (local development)
USER_SERVICE_URL=http://localhost:8001
DECLARATION_SERVICE_URL=http://localhost:8002
FORM_SERVICE_URL=http://localhost:8003

# Frontend
FRONTEND_URL=http://localhost:3000

